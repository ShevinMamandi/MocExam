    "concurrently": "^7.1.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "node-fetch": "^3.2.4",
    "react": "^18.1.0",
